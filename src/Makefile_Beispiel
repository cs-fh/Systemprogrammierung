
all: ledbar.o studiverwaltung.o pruefungen.o mainled mainpruef
ledbar.o:
	gcc -ansi -Wall -pedantic ledbar.c -o ledbar.o -c
studiverwaltung.o:
	gcc -ansi -Wall -pedantic studiverwaltung.c -o studiverwaltung.o -c
pruefungen.o:
	gcc -ansi -Wall -pedantic pruefungen.c -o pruefungen.o -c
mainled:
	gcc ledbar.o mainled.c -lwiringPi -lpthread -o mainled
mainpruef:
	gcc ledbar.o studiverwaltung.o pruefungen.o mainpruef.c -lwiringPi -lpthread -o mainpruef
clean:
	rm -f *.o
	rm -f mainled mainpruef
